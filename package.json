{
	"name": "@gramio/broadcast",
	"version": "0.0.2",
	"description": "Auto answer callback query plugin for GramIO",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"type": "module",
	"author": "kravets",
	"license": "MIT",
	"readme": "https://gramio.dev/plugins/official/auto-answer-callback-query",
	"homepage": "https://github.com/gramiojs/auto-answer-callback-query",
	"keywords": [
		"gramio",
		"gramio-plugin",
		"auto-answer-callback-query",
		"callback-query",
		"answer-callback-query",
		"telegram",
		"telegram-bot-api"
	],
	"scripts": {
		"prepublishOnly": "bunx pkgroll",
		"lint": "bunx @biomejs/biome check src",
		"lint:fix": "bun lint --write"
	},
	"peerDependencies": {
		"gramio": ">=0.0.39 <1.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/bun": "^1.1.14",
		"gramio": "^0.1.3",
		"ioredis": "^5.4.1",
		"jobify": "^0.1.3",
		"pkgroll": "^2.5.1",
		"typescript": "^5.7.2"
	},
	"files": ["dist"]
}
